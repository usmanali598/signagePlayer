{
  "name": "seesignage",
  "version": "1.0.0",
  "description": "signage player for running images and mp4 videos",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix ../client",
    "client": "npm run dev --prefix ../client/",
    "build": "tsc -p .",
    "dev-start": "nodemon build/index.js",
    "start":"NODE_PATH=./build node build/index.js",
    "test": "jest --testPathPattern=src/__test__",
    "dev-cli": "concurrently \"npm run dev-start\" \"npm run client\"",
    "dev": "nodemon --watch src --exec ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.2",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "mongoose": "^6.8.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/parser": "^5.47.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
